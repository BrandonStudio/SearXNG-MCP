{
  "name": "searxng-mcp-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for searxng-mcp packages",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -w -r run build",
    "dev": "pnpm -w -r run dev",
    "start:example": "pnpm --filter searxng-mcp start"
  },
  "keywords": ["mcp", "searxng", "search", "metasearch", "model-context-protocol"],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
