{
  "name": "@bs-mcps/searxng",
  "version": "0.2.7",
  "description": "MCP server for SearXNG",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "bs-mcps-searxng": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "test": "pwsh -File tests/run_test.ps1",
    "test:cf": "pwsh -File tests/run_test_cf.ps1",
    "wrangler:dev": "wrangler dev src/cf-worker.ts",
    "wrangler:deploy": "wrangler deploy --keep-vars"
  },
  "keywords": [
    "ai",
    "mcp",
    "searxng",
    "search",
    "metasearch",
    "model-context-protocol"
  ],
  "author": "BrandonStudio",
  "license": "Apache-2.0",
  "homepage": "https://github.com/BrandonStudio/SearXNG-MCP",
  "bugs": {
    "url": "https://github.com/BrandonStudio/SearXNG-MCP/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrandonStudio/SearXNG-MCP.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    "./searxng": {
      "types": "./dist/searxng.d.ts",
      "import": "./dist/searxng.js",
      "require": "./dist/searxng.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.js"
    }
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "agents": "^0.2.35",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251205.0",
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "wrangler": "^4.53.0"
  },
  "pnpm": {
    "overrides": {
      "@modelcontextprotocol/sdk": ">=1.25.2",
      "qs": ">=6.14.1"
    }
  }
}
